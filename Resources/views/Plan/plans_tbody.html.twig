<tbody class="plans" >
	{% for plan in plans %}
		<tr id="plan_{{ plan.id }}" class="{% if plan.isDisabled %}disabled{% elseif plan.isInactive %}inactive{% elseif plan.isDefault %}default{%endif %}" >
			<td class="number" >{{ plan.rank }}</td>
			<td><a href="{{ path('dzangocart_subscription_plan', { 'id': plan.id }) }}" >{{ plan.name }}</a></td>
			<td>{% if plan.start %}{{ plan.start|date('Y-m-d H:i') }}{% endif%}</td>
			<td>{% if plan.finish %}{{ plan.finish|date('Y-m-d H:i') }}{% endif%}</td>
			<td class="amount" ></td>
			<td class="actions" >{% include "DzangocartSubscriptionBundle:Plan:actions.html.twig"%}</td>
		</tr>
	{% endfor %}
</tbody>


